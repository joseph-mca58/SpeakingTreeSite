@model SpeakingTree.Models.Enquiry

@{
    ViewBag.Title = "Enquiry";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Enquiry</h2>
@using (Html.BeginForm("Enquiry", "Home", FormMethod.Post, new { enctype = "multipart/form-data", id = "enquryForm", @class = "form-horizontal" }))
{
    @Html.AntiForgeryToken()
    <p></p>

    <div class="row">
        <div class="col-sm-2">Name </div>
        <div class="col-sm-10">
            @Html.TextBoxFor(model => model.Name)

        </div>
    </div>
    <p></p>

    <div class="row">
        <div class="col-sm-2">School </div>
        <div class="col-sm-10">
            @Html.TextBoxFor(model => model.School)

        </div>
    </div>
    <p></p>

    <div class="row">
        <div class="col-sm-2">Email Id </div>
        <div class="col-sm-10">
            @Html.TextBoxFor(model => model.EmailId)

        </div>
    </div>
    <p></p>
    <div class="row">
        <div class="col-sm-2">Contact Number </div>
        <div class="col-sm-4">
            @Html.TextBoxFor(model => model.ContactNo)

        </div>
    </div>
    <p></p>
    <div class="row">
        <div class="col-sm-2">Query </div>
        <div class="col-sm-4">
            @Html.TextBoxFor(model => model.Query)

        </div>
    </div>
    <p></p>
    <p></p>
    <div class="row">
        <div class="col-sm-2"></div>

        <div class="col-sm-10">
            <button class="btn btn-primary btn-sm" id="btn-impMeeting" type="submit" style="margin-left: 5px; margin-right: 5px;">Submit</button>
        </div>

    </div>
}
